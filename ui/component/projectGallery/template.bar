<div class="swiper-wrapper">
	{{#iterate data}}
	<a is="ui-link"
	 href={{@key}}
	 class="swiper-slide">
		<div 
		 class="parallax-bg" 
		 style="background-image:url({{url}})"
		 data-swiper-parallax="-38%"
		 ></div>
		<h1 class="[ heading heading_1 ]">
		 {{capitalize (stem @key)}}</h1>
		{{!--img-->
			<img src="{{url}}" class="project--item=img" alt="{{stem @key}}"/>
			!--}}
	</a>
	{{/iterate}}
</div>
<swpr-nav></swpr-nav>